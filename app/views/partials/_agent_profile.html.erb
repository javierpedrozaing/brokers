<div class="form-outline mb-4" style="display:none">
  <%= form.label "Registerd in", class: "form-label" %><br />
  <%= form.text_field :created_at, value: current_user.created_at.strftime("%Y-%m-%d"), readonly: true, class: "form-control" %>    
</div>

<div class="form-outline mb-4">
  <%= form.label :address, class: "form-label" %><br />
  <%= form.text_field :address, value: @agent.address, autofocus: true, autocomplete: "Address", class: "form-control" %>  
</div>

<div class="form-outline mb-4">
  <%= form.label :country, class: "form-label" %><br />
  <%= form.select(:country, @countries, { selected:  @current_country }, {:class => 'form-control' }) %>  
</div>

<div class="form-outline mb-4">
  <%= form.label :state, class: "form-label" %><br />
  <%= form.select(:state, [], { selected: @current_state }, { :class => 'form-control' }) %>    
</div>

<div class="form-outline mb-4">
  <%= form.label :city, class: "form-label" %><br />
  <%= form.select(:city, [], { selected: @current_city }, { :class => 'form-control' } ) %>
</div>

<div class="form-outline mb-4">
  <%= form.label "Company zip code", class: "form-label" %><br />
  <%= form.text_field :zip_code, value: @agent.zip_code, autofocus: true, autocomplete: "zip_code", class: "form-control" %>  
</div>