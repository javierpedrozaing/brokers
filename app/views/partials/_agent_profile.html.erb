<div class="form-outline mb-4">
  <%= form.label :birthday, class: "form-label" %><br />
  <%= form.date_field :birthday, autofocus: true, autocomplete: "Phone", class: "form-control" %>  
  <%= form.date_field :birthday, value: @agent.birthday unless @agent.nil? %>  
</div>

